{
  "expo": {
    "name": "TravelTies",
    "slug": "TravelTies-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/travelties-logo.png",
    "scheme": "com.travelties.travelties",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "usesIcloudStorage": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.travelties.travelties",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSCameraUsageDescription": "TravelTies need access to your camera to take profile pictures",
        "NSPhotoLibraryUsageDescription": "TravelTies need access to your photo library to select photos"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.travelties.travelties",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-web-browser",
      "expo-document-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7a21ffe9-a591-4d42-a9d7-3c712b3a96c9"
      }
    },
    "owner": "vickylin"
  }
}
